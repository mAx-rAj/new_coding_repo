class employee1():
    def __init__(self,name,age,salary):
        self.name=name
        self.age=age
        self.salary=salary
    def display(self):
        print(self.name)
        print(self.age)
        print(self.salary)
class childemployee(employee1):
    def __init__(self,name,age,salary,ID):
        self.name=name
        self.age=age
        self.salary=salary
        self.id=ID
emp1=employee1('abc',22,10000)
emp1.display()

#include <stdio.h>

int main()
{
        int i=34;
        char c = 'c';
        int *ptr=&i;
        int *d= &c; 
    printf("The  value of d is %u\n",d);
    printf("The value of ptr is %u\n",ptr); 
    ptr++;
    printf("The value of ptr is %u\n", ptr);
    ptr=ptr+ 4;
    printf("The value of ptr is  %u\n", ptr);
    d=d-1;
    printf("The value  of d is  %u\n",d);
    return 0;
}
